 main {

 var src = new IloOplModelSource("model01.mod");
 var def = new IloOplModelDefinition(src);
 var cplex = new IloCplex();
 var model = new IloOplModel(def,cplex);
 var data = new IloOplDataSource("basicTest-minHours01.dat");
 model.addDataSource(data);
 model.generate();
 cplex.epgap=0.01;

if (cplex.solve()) {
 	writeln("basicTest-minHours01.dat ------------- SOLVED");
} else {
	writeln("basicTest-minHours01.dat ------------- FAILED");

} 	
 data.end();
 
 var data = new IloOplDataSource("basicTest-minHours02.dat");
 model.addDataSource(data);
 model.generate();
 cplex.epgap=0.01;
 if (cplex.solve()) {
 	writeln("basicTest-minHours02.dat ------------- SOLVED");
} else {
	writeln("basicTest-minHours02.dat ------------- FAILED");

} 	
 data.end();
 
 
 
 model.end();
 def.end();
 cplex.end();
 src.end(); 
 
/* if (cplex.solve()) {
 	writeln("Max load " + cplex.getObjValue() + "%");
	for (var c=1;c<=model.nCPUs;c++) {
		var load=0;
		for (var t=1;t<=model.nTasks;t++)
			load+=(model.rt[t]* model.x_tc[t][c]);
		load = (1/model.rc[c])*load;
	writeln("CPU " + c + " loaded at " + load + "%");
	}
 } else {
 	writeln("Not solution found");
 }


*/
};